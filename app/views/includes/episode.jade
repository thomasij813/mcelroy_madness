li.episode(audio="#{program.audio_download}")
  -if (program.feed_type === 'audio/podcast')
    .img_container
      img(src="#{program.image_url}")
      .img_overlay
        i.fa.fa-play.audio_icon
    .program_content
      .program_title #{program.title}
      .program_date #{program.disp_date}
  -else
    .video_img_container(link="#{program.video_link}")
      img(src="#{program.image_url}")
      .video_img_overlay
        i.fa.fa-play-circle-o
